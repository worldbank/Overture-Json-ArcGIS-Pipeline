# .env.example - This file is committed to Git as documentation
# Copy this to .env and fill in your actual values

# ArcGIS Online Configuration
AGOL_PORTAL_URL=https://your-org.maps.arcgis.com
AGOL_USERNAME=your_username
AGOL_PASSWORD=your_secure_password_here

# Overture Maps Configuration  
OVERTURE_S3_REGION=us-west-2
OVERTURE_RELEASE=2025-07-23.0/  # Latest release version

# Processing Configuration
DUCKDB_MEMORY_LIMIT=16GB
DUCKDB_THREADS=8

# Temporary File Management
TEMP_RETENTION_HOURS=24    # How long to keep temp files (hours)
TEMP_SIZE_WARNING_GB=10    # Log warning when temp folder exceeds this size
TEMP_SIZE_LIMIT_GB=40      # Hard limit - trigger emergency cleanup

# Local Data Dump
# OVERTURE_DUMP_PATH=/path/to/your/dumps #uncomment and change if you want to change the default download locations